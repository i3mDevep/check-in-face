{
  "name": "check-in-face-frontend",
  "$schema": "node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "./src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/check-in-face-frontend"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "check-in-face-frontend:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "check-in-face-frontend:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "check-in-face-frontend:build:production",
          "hmr": false
        }
      }
    },
    "generate-graphql-types": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "pnpm apollo service:download --endpoint=https://oe5p6maoovfzjdfd7kxva7ali4.appsync-api.us-east-1.amazonaws.com/graphql graphql-schema.json --header=\"x-api-key: da2-lxzdk2xghzevrpfhpne5nu3nse\"",
          "pnpm apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --tagName=gql"
        ]
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "check-in-face-frontend:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "check-in-face-frontend:build:development"
        },
        "production": {
          "buildTarget": "check-in-face-frontend:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["coverage/check-in-face-frontend"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "coverage/check-in-face-frontend"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["./src/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "check-in-face-frontend:build"
      }
    }
  },
  "tags": []
}
